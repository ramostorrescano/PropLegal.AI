{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "^/favicon\\.ico$",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest\\.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/asset-manifest\\.json$",
      "dest": "/asset-manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
